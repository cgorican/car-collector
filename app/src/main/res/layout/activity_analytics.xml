        android:id="@+id/subtitle"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/margin_left_right"
        android:textColor="@color/gray"
        android:textSize="@dimen/text_size_value"
